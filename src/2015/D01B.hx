	static function process() {
		var floor = 0;
		var instructions = input.split("");
		for (i in 0...instructions.length) {
			switch (instructions[i]) {
				case "(":
					floor++;
				case ")":
					floor--;
				case _:
					throw "invalid instruction";
			}
			if (floor == -1) {
				// reached basement
				Sys.println('instruction = ${i+1}');
				return;
			}
		}
	}

